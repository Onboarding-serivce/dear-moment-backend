SPRING_APPLICATION_NAME= # 애플리케이션 이름을 설정합니다. (예: demo)
  
  # Database Configuration for Production
SPRING_DATASOURCE_URL= # 데이터베이스 연결 URL (예: jdbc:oracle:thin:@localhost:1521/FREEPDB1)
SPRING_DATASOURCE_USERNAME= # 데이터베이스 사용자 이름 (예: spring_user)
SPRING_DATASOURCE_PASSWORD= # 데이터베이스 비밀번호 (예: spring_password)
SPRING_DATASOURCE_DRIVER_CLASS_NAME= # 데이터베이스 드라이버 클래스 이름 (예: oracle.jdbc.OracleDriver)
  
  # JPA Configuration
SPRING_JPA_HIBERNATE_DDL_AUTO= # Hibernate DDL 자동화 설정 (예: update, validate, create, create-drop)
SPRING_JPA_SHOW_SQL= # SQL 쿼리를 출력할지 여부 (true/false)
SPRING_JPA_DATABASE_PLATFORM= # JPA에서 사용할 데이터베이스 플랫폼 (예: org.hibernate.dialect.OracleDialect)